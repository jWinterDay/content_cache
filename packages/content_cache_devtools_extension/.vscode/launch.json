    "version": "0.2.0",
    "configurations": [
        {
            "name": "devtools_extension + simulated environment",
            "request": "launch",
            "type": "dart",
            "cwd": "packages/content_cache_devtools_extension",
            "args": [
                "--dart-define=use_simulated_environment=true"
            ],
        },
        {
            "name": "devtools_extension + simulated environment (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=use_simulated_environment=true"
            ],
        },
        {
            "name": "devtools_extension + simulated environment (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=use_simulated_environment=true"
            ],
        }
    ]
}